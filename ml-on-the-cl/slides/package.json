{
  "name": "ml-on-the-cl",
  "version": "1.0.0",
  "author": "Kylie Stewart <kylieastewart@gmail.com>",
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "3.1.1",
    "spectacle": "^5.7.0"
  },
  "scripts": {
    "build": "npm run clean && react-scripts build ./src",
    "clean": "rm -rf ./build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "prettier": "cat .gitignore .prettierignore > .gitprettierignore && prettier --write --config .prettierrc \"**/*.{js,scss,json}\" --ignore-path .gitprettierignore",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "prettier": "^1.13.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
